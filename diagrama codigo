#include <iostream>
using namespace std;

int main()
{
    int X=1;
   
    while (X<=5)
    {
    if (X%3==0)
             {X=X+2;}
    else {
        cout<<"X="<<X<<endl;
        X=X+2;
    }
    }
   
   
    return 0;
}
