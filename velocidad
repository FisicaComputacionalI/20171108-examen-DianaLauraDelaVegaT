#include <iostream>
using namespace std;

int main (){
  float d;
  
  float t;

   cout<<"la diatancia d es:"<<endl;
   cin>> d;
  
   cout<<"el tiempo t (debe ser mayor que cero) es:"<<endl;
   cin>> t;
  
   cout<<"La velocidad de la particula es: "<< d/t<<endl;
   
   
  return 0;
}
